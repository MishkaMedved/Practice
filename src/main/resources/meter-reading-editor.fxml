<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.mixail.Controllers.MeterReadingEditorController">
    <AnchorPane prefHeight="300.0" prefWidth="400.0">
      <children>
      
          <Label layoutX="18.0" layoutY="26.0" text="Выберите прибор учета:" />
          <ComboBox fx:id="meterComboBox" layoutX="163.0" layoutY="22.0" promptText="Выберите прибор учета" />
      
          <Label layoutX="22.0" layoutY="60.0" text="Введите показание:" />
          <TextField fx:id="readingField" layoutX="176.0" layoutY="56.0" promptText="Показание" />
      
          <Button layoutX="101.0" layoutY="104.0" onAction="#addReading" prefHeight="45.0" prefWidth="150.0" text="Добавить показание" />
      </children>
    </AnchorPane>
</VBox>
